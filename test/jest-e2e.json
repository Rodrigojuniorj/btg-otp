{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "setupFiles": ["<rootDir>/test/setup-e2e.ts"],
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "testTimeout": 60000,
  "setupFilesAfterEnv": ["<rootDir>/test/setup-e2e.ts"],
  "maxWorkers": 1,
  "maxConcurrency": 1
}
